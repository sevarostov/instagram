{% extends 'base.html.twig' %}

{% block title %} Embed {% endblock %}
{% block body %}
<div class="container">
	<div class="row">
		<div class="col-lg-6 col-lg-offset-3">
			<h1 style="margin-top: 70px;">Instagram</h1>
			{% if data|length %}
				{% for item in data %}
					<div class="message" style="margin: 10px 10px 0 0;">
						<div class="icon">
							{{ item['html']|raw }}
						</div>
					</div>
				{% endfor %}
			{% endif %}
		</div>
	</div>
</div>

{% endblock %}